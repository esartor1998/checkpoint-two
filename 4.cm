/* nCr in c-. to check function calls exprs etc. */

int fact(int num) {
	int k; int i;
	k = 1; i = 1; /*ee ur*/
	if (num == 0) {
		return(k);
	}
	else {
		while (i <= num) { /*nestin*/
			k = k * i;
			i = i + 1;
		}
	}
	return k;
}

int k() {
    int x = 3;
    int x[3+3];
}

void main(void)	{
	int n; int r; int ncr_var;
	n = input();
	r = input();
	/* C(n,r) = n! / ( r!(n - r)! ), for 0 <= r <= n */
	ncr_var = fact(n) / (fact(r) * fact(n - r));
	output(n, r, ncr_var);	
}